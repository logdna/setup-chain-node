{
  "name": "@answerbook/logdna-test-setup-chain",
  "version": "0.0.0",
  "description": "Base class for implement seed and resource data for test suites",
  "main": "index.js",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "lint": "eslint .",
    "commitlint": "commitlint-logdna --from=origin/master",
    "pretest": "npm run lint && npm run commitlint",
    "test": "tap; cat output.tap | ./node_modules/.bin/tap-mocha-reporter xunit > coverage/test.xml",
    "tap": "tap"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/answerbook/logdna-test-setup-chain"
  },
  "keywords": [
    "testing"
  ],
  "author": "Eric Satterwhite <eric.satterwhite@logdna.com>",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/answerbook/logdna-test-setup-chain/issues"
  },
  "eslintConfig": {
    "extends": "eslint-config-logdna",
    "root": true,
    "ignorePatterns": [
      "node_modules/",
      "coverage/"
    ]
  },
  "homepage": "https://github.com/answerbook/logdna-test-setup-chain#readme",
  "devDependencies": {
    "@answerbook/commitlint-config-logdna": "^1.2.0",
    "casual": "^1.6.2",
    "eslint": "^7.4.0",
    "eslint-config-logdna": "^4.0.0",
    "moment": "^2.27.0",
    "tap": "^14.10.7"
  },
  "tap": {
    "100": true,
    "ts": false,
    "esm": false,
    "jsx": false,
    "browser": false,
    "reporter": "classic",
    "output-file": "output.tap",
    "check-coverage": true,
    "files": [
      "test/**/*.js"
    ],
    "coverage-report": [
      "text",
      "text-summary",
      "json",
      "html"
    ],
    "nyc-arg": [
      "--exclude=test/",
      "--exclude=coverage/"
    ]
  }
}
