{
  "name": "@answerbook/logdna-test-setup-chain",
  "version": "0.0.0",
  "description": "Base class for implement seed and resource data for test suites",
  "main": "index.js",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "test": "tap;cat $(find .tap -type f -name '*.tap') | ./node_modules/.bin/tap-mocha-reporter xunit > coverage/test.xml",
    "tap": "tap"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/answerbook/logdna-test-setup-chain"
  },
  "keywords": [
    "testing"
  ],
  "author": "Eric Satterwhite <eric.satterwhite@logdna.com>",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/answerbook/logdna-test-setup-chain/issues"
  },
  "homepage": "https://github.com/answerbook/logdna-test-setup-chain#readme",
  "devDependencies": {
    "casual": "^1.6.2",
    "tap": "^14.10.7"
  },
  "tap": {
    "ts": false,
    "esm": false,
    "jsx": false,
    "browser": false,
    "branches": 98,
    "lines": 98,
    "functions": 98,
    "statements": 98,
    "reporter": "classic",
    "output-dir": ".tap",
    "check-coverage": true,
    "files": [
      "test/**/*.js"
    ],
    "coverage-report": [
      "text",
      "text-summary",
      "json",
      "html"
    ],
    "nyc-arg": [
      "--exclude=test/",
      "--exclude=coverage/"
    ]
  }
}
